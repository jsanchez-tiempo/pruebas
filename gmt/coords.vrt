<OGRVRTDataSource>
  <OGRVRTLayer name="output">
    <SrcDataSource relativeToVRT="1">_FILECOORDS</SrcDataSource>
    <SrcLayer>coords</SrcLayer>
    <GeometryType>wkbPoint</GeometryType>
    <LayerSRS>WGS84</LayerSRS>
    <GeometryField encoding="PointFromColumns" x="x" y="y"/>
    <Field name="x" type="Real"/>
    <Field name="y" type="Real"/>
    <Field name="z" type="Integer"/>
  </OGRVRTLayer>
</OGRVRTDataSource>
